{
    "name": "mdjward/behat-env-var-extension",
    "description": "Behat extension to make environment variables available as container parameters",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Matt Ward",
            "email": "dev@mattdw.co.uk"
        }
    ],
    "keywords": [
        "behat",
        "extension",
        "bdd",
        "dependency-injection",
        "service-container",
        "environment-variables",
        "environment",
        "env"
    ],
    "homepage": "https://github.com/mdjward/behat-env-var-extension",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "behat/behat": "~3.0"
    },
    "require-dev": {
        "symfony/process": "2.*|3.*",
        "symfony/filesystem": "2.*|3.*",
        "phpunit/phpunit": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Mdjward\\Behat\\EnvVarExtension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mdjward\\Behat\\EnvVarExtension\\": [
                "tests/unit/",
                "tests/integration/"
            ]
        }
    }
}
