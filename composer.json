{
    "name": "mdjward/behat-env-var-extension",
    "authors": [
        {
            "name": "Matt Ward",
            "email": "dev@mattdw.co.uk"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "behat/behat": "~3.0"
    },
    "require-dev": {
        "symfony/process": "2.*|3.*",
        "symfony/filesystem": "2.*|3.*",
        "phpunit/phpunit": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Mdjward\\Behat\\EnvVarExtension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mdjward\\Behat\\EnvVarExtension\\": [
                "tests/unit/",
                "tests/integration/"
            ]
        }
    }
}
